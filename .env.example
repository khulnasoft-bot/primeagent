# Description: Example of .env file
# Usage: Copy this file to .env and change the values
#        according to your needs
#        Do not commit .env file to git
#        Do not change .env.example file

# Config directory
# Directory where files, logs and database will be stored
# Example: PRIMEAGENT_CONFIG_DIR=~/.primeagent
PRIMEAGENT_CONFIG_DIR=

# Save database in the config directory
# Values: true, false
# If false, the database will be saved in Primeagent's root directory
# This means that the database will be deleted when Primeagent is uninstalled
# and that the database will not be shared between different virtual environments
# Example: PRIMEAGENT_SAVE_DB_IN_CONFIG_DIR=true
PRIMEAGENT_SAVE_DB_IN_CONFIG_DIR=

# Database URL
# Postgres example: PRIMEAGENT_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/primeagent
# SQLite example:
PRIMEAGENT_DATABASE_URL=sqlite:///./primeagent.db

# Alembic logs path flag. If set to true, Alembic will log to stdout.
PRIMEAGENT_ALEMBIC_LOG_TO_STDOUT=False


# mem0 creates a directory 
# for chat history, vector stores, and other artifacts
# its default path is ~/.mem0.
# we can change this path with 
# environment variable "MEM0_DIR"
# Example: MEM0_DIR=/tmp/.mem0

# composio creates a cache directory 
# for file uploads/downloads.
# its default path is ~/.composio
# we can change this path with 
# environment variable "COMPOSIO_CACHE_DIR"
# Example: COMPOSIO_CACHE_DIR=/tmp/.composio


# Database connection retry
# Values: true, false
# If true, the database will retry to connect to the database if it fails
# Example: PRIMEAGENT_DATABASE_CONNECTION_RETRY=true
PRIMEAGENT_DATABASE_CONNECTION_RETRY=false

# Cache type
PRIMEAGENT_LANGCHAIN_CACHE=SQLiteCache

# Server host
# Example: PRIMEAGENT_HOST=localhost
PRIMEAGENT_HOST=

# Worker processes
# Example: PRIMEAGENT_WORKERS=1
PRIMEAGENT_WORKERS=

# Server port
# Example: PRIMEAGENT_PORT=7860
PRIMEAGENT_PORT=

# Logging level
# Example: PRIMEAGENT_LOG_LEVEL=critical
PRIMEAGENT_LOG_LEVEL=

# Path to the log file
# Example: PRIMEAGENT_LOG_FILE=logs/primeagent.log
PRIMEAGENT_LOG_FILE=

# Time/Size for log to rotate
# Example: PRIMEAGENT_LOG_ROTATION=‘10 MB’/‘1 day’
PRIMEAGENT_LOG_ROTATION=

# Path to the frontend directory containing build files
# Example: PRIMEAGENT_FRONTEND_PATH=/path/to/frontend/build/files
PRIMEAGENT_FRONTEND_PATH=

# Whether to open the browser after starting the server
# Values: true, false
# Example: PRIMEAGENT_OPEN_BROWSER=true
PRIMEAGENT_OPEN_BROWSER=

# Whether to remove API keys from the projects saved in the database
# Values: true, false
# Example: PRIMEAGENT_REMOVE_API_KEYS=false
PRIMEAGENT_REMOVE_API_KEYS=

# Whether to use RedisCache or ThreadingInMemoryCache or AsyncInMemoryCache
# Values: async, memory, redis
# Example: PRIMEAGENT_CACHE_TYPE=memory
# If you want to use redis then the following environment variables must be set:
# PRIMEAGENT_REDIS_HOST (default: localhost)
# PRIMEAGENT_REDIS_PORT (default: 6379)
# PRIMEAGENT_REDIS_DB (default: 0)
# PRIMEAGENT_REDIS_CACHE_EXPIRE (default: 3600)
PRIMEAGENT_CACHE_TYPE=

# Set PRIMEAGENT_AUTO_LOGIN to false if you want to disable auto login
# and use the login form to login. PRIMEAGENT_SUPERUSER and PRIMEAGENT_SUPERUSER_PASSWORD
# must be set if AUTO_LOGIN is set to false
# Values: true, false
PRIMEAGENT_AUTO_LOGIN=

# SET PRIMEAGENT_ENABLE_SUPERUSER_CLI to false to disable
# superuser creation via the CLI
PRIMEAGENT_ENABLE_SUPERUSER_CLI=

# Superuser username
# Example: PRIMEAGENT_SUPERUSER=admin
PRIMEAGENT_SUPERUSER=

# Superuser password
# Example: PRIMEAGENT_SUPERUSER_PASSWORD=123456
PRIMEAGENT_SUPERUSER_PASSWORD=

# API Key Source
# Controls how API keys are validated for the x-api-key header
# Values: db, env
# - db (default): Validates against API keys stored in the database
# - env: Validates against the PRIMEAGENT_API_KEY environment variable
# Example: PRIMEAGENT_API_KEY_SOURCE=db
PRIMEAGENT_API_KEY_SOURCE=

# API Key (only used when PRIMEAGENT_API_KEY_SOURCE=env)
# The API key to use for authentication when API_KEY_SOURCE is set to 'env'
# This allows injecting a pre-defined API key via environment variables
# (useful for Kubernetes Secrets, CI/CD pipelines, etc.)
# Example: PRIMEAGENT_API_KEY=your-secure-api-key
PRIMEAGENT_API_KEY=

# Should store environment variables in the database
# Values: true, false
PRIMEAGENT_STORE_ENVIRONMENT_VARIABLES=

# Should enable the MCP composer feature in MCP projects
# Values: true, false
# Default: true
PRIMEAGENT_MCP_COMPOSER_ENABLED=

# STORE_URL
# Example: PRIMEAGENT_STORE_URL=https://api.primeagent.store
# PRIMEAGENT_STORE_URL=

# DOWNLOAD_WEBHOOK_URL
#
# PRIMEAGENT_DOWNLOAD_WEBHOOK_URL=

# LIKE_WEBHOOK_URL
#
# PRIMEAGENT_LIKE_WEBHOOK_URL=

# Value must finish with slash /
#BACKEND_URL=http://localhost:7860/
BACKEND_URL=
